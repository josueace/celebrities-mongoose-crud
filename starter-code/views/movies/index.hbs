<h2>List of Movies</h2>
<ul>
{{#each movies}}

  <a href="/movies/{{this._id}}"> <li>{{this.title}}</li></a>
<form method="post" action="/movies/{{this._id}}/delete">
  <button type="submit" >Delete</button>

    
</form>
<br>
  <a href="/movies/{{this._id}}/edit">Edit Movie</a>
{{/each}}
</ul>
<a href="/movies/new">Add new Movie</a>